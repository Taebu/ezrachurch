.join_comment div{border-bottom:1px solid #d5d5d5;border-top:1px solid #d5d5d5;background:#edeff4;color:#333387;padding:7px 14px 11px 14px;font-weight:900;width:567px;}
.join_comment ol{padding:24px 24px 24px 24px;line-height:1.8;margin:0;}
.join_comment li{list-style: url('img/list.gif') ;}
/*-28 => calc(100%-28px)*/
.manager_comment{width:100%;border-top:3px solid #549ad9;border-bottom:1px solid #d5e4f1;border-left:1px solid #d5e4f1;border-right:1px solid #d5e4f1;margin-top:25px;background:white}
.manager_comment div{border-bottom:1px solid #d5e4f1;border-top:1px solid #d5e4f1;background:#f3f8fc;color:#333387;padding:7px 14px 11px 14px;font-weight:900; width: calc(100% - 28px);width: -moz-calc(100% - 28px);width: -webkit-calc(100% - 28px);}
.manager_comment ol{padding:24px 24px 24px 24px;line-height:1.8;margin:0;}
.manager_comment ul{padding:24px 24px 24px 24px;line-height:1.8;margin:0;}
.manager_comment li{list-style: url('img/list.gif') ;}
.join_comment .red{color:red;}

